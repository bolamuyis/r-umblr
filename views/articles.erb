<div class="contentWrapper">
<% @posts.each do |post| %>
  <div class="articleCopy">
    <p class="userslistpage"><%= "Title: " %><%= post.title %>
    <br /><%= "By: " %><%= post.first_name %><%= " " %><%= post.last_name %></p>
    <p class="userslistpage"><%= post.content %></p>
    <p class="userslistpage"><%= post.datetime %></p>
  </div>
  <div class="articleImg">
    <img src="<%= post.image_url %>">
  </div>
  <div class="postDiv"></div>
<% end %>
</div>